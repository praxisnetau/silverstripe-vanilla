{
  "name": "sunnysideup/silverstripe-vanilla",
  "type": "silverstripe-theme",
  "description": "An alternative default theme for SilverStripe.",
  "homepage": "https://github.com/praxisnetau/silverstripe-vanilla",
  "keywords": [
    "silverstripe",
    "vanilla",
    "theme"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Colin Tucker",
      "role": "Developer",
      "email": "colin@praxis.net.au",
      "homepage": "http://www.praxis.net.au"
    }
  ],
  "require": {
    "silverstripe/framework": "^4.0 || ^5.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "installer-name": "vanilla",
    "expose": [
      "production",
      "css",
      "images",
      "js"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
